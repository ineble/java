package y2022_06_09;

public class Member {
	public String id;

	public Member(String id) {
		this.id = id;
	}

	@Override
	public boolean equals(Object obj) { //오브젝트 타입이 같은지 물어보는 equals
		if(obj instanceof Member) {  
		Member member = (Member)obj;
		if(id.equals(member.id)) { // 문자열이 같은지 물어보는 equals
			return true;
			}
		}
		return false;
	}
	
	
	
	
	
}
