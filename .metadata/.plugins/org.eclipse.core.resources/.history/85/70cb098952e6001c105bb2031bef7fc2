package y2022_06_07;

public class SmartTelevisionExample {

	public static void main(String[] args) {
		SmartTelevision tv = new SmartTelevision();
		tv.turnOn();
		tv.setVolume(8);
		tv.turnOff();
		tv.search("mbc");
		RemoteControl rc = tv;
		rc.turnOn();
		rc.setVolume(8);
		rc.turnOff();
		rc.search("mbc");
		Searchable sc = tv;
		tv.turnOn();
		tv.setVolume(8);
		tv.turnOff();
		tv.search("mbc");
	}

}
