package y2022_06_08;

public class A {
	A(){
		System.out.println("A 객체가 생생됨");
	}
	
	class B {
		B(){
			System.out.println("B 객체가 생성됨");
		}
		int field1;
		void method1() {}
	}
	
	static class C {
		C(){
			System.out.println("C 객체가 생성됨");
		}
		int field1;
		static int field2;
		void method1() {}
		static method2() {}
	}
	
	void method() {
		class D{
			D(){
				System.out.println("D 객체가 생성됨");
			}
		}
	}
}
