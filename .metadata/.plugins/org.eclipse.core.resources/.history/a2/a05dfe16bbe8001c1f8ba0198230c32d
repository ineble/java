package practice;

public class Main {
	public static void main(String[] args) {
		int x = 0;
		int y = 0;
		while(true) {
			x = (int)Math.random()*6+1;
			
			y = (int)Math.random()*6+1;
			System.out.print("(" + x + "," + y + ") ");
			if((x + y) == 5) {
				break;
			} else {
				
			}
			
		}
	}
}

