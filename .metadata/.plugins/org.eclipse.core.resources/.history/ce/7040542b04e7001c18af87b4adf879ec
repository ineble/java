package y2022_06_07;

public class SmartTelevisionExample {

	public static void main(String[] args) {
		SmartTelevision tv = new SmartTelevision(); //객체 생성
		tv.turnOn();
		tv.setVolume(8);
		tv.turnOff();
		tv.search("mbc");
		RemoteControl rc = tv; //객체 생성
		rc.turnOn();
		rc.setVolume(8);
		rc.turnOff();
		Searchable sa = tv; //객체 생성
		sa.search("sbs");
	}

}
