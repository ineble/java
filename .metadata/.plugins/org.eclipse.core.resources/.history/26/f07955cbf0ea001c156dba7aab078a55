package y2022_06_13;

public class MathRandomExample {

	public static void main(String[] args) {
		int v1 = Math.abs(-5);
		double v2 = Math.abs(-3.14);
		System.out.println("v1 : " + v1);
		System.out.println("v2 : " + v2);
		
		double v3 = Math.ceil(5.3);
		double v3 = Math.ceil(-5.3);
		

	}

}
