package practice;

public class Main {
	public static void main(String[] args) {
		while(true) {
			int x = (int)Math.random()*6;
			System.out.println(x);
			
			int y = (int)Math.random()*6;
			System.out.println(y);
			if((x + y) == 5) {
				break;
			} else {
				System.out.print("(" + x + "," + y + ") ");
			}
			
		}
	}
}

