package practice;

import java.util.Scanner;

public class Main {
	public static void main(String[] args) {
		while(true) {
			int x = (int)Math.random()*6+1;
			System.out.println(x);
			
			int y = (int)Math.random()*6+1;
			if((x + y) == 5) {
				break;
			} else {
				System.out.print("(" + x + "," + y + ") ");
			}
			
		}
	}
}

